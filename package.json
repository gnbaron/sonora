{
  "name": "sonora",
  "version": "0.0.1",
  "description": "Academic project of a music streaming service developed using Elixir, Phoenix and React w/ Redux.",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest web/static/js",
    "test.coverage": "jest --coverage web/static/js",
    "test.watch": "jest --watch web/static/js"
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/immutable/",
      "<rootDir>/node_modules/lodash"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guilhermefloriani/sonora.git"
  },
  "keywords": [
    "music"
  ],
  "author": "Guilherme Floriani Baron - Anderson Balena Fernandes",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^5.8.0",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.1.0",
    "eslint": "~2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.0",
    "marked": "^0.3.5",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "jest": "^0.1.40",
    "jest-cli": "^0.9.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
